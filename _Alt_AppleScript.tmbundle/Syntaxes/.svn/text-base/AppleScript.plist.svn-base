							<key>name</key>
							<string>punctuation.separator.array.applescript</string>
						</dict>
						<dict>
							<key>include</key>
							<string>#inline</string>
						</dict>
					</array>
				</dict>
				<dict>
					<key>begin</key>
					<string>((?&lt;=application )"|(?&lt;=app )")</string>
					<key>captures</key>
					<dict>
						<key>1</key>
						<dict>
							<key>name</key>
							<string>punctuation.definition.string.applescript</string>
						</dict>
					</dict>
					<key>end</key>
					<string>(")</string>
					<key>name</key>
					<string>string.quoted.double.application-name.applescript</string>
					<key>patterns</key>
					<array>
						<dict>
							<key>match</key>
							<string>\\.</string>
							<key>name</key>
							<string>constant.character.escape.applescript</string>
						</dict>
					</array>
				</dict>
				<dict>
					<key>begin</key>
					<string>(")</string>
					<key>captures</key>
					<dict>
						<key>1</key>
						<dict>
							<key>name</key>
							<string>punctuation.definition.string.applescript</string>
						</dict>
					</dict>
					<key>end</key>
