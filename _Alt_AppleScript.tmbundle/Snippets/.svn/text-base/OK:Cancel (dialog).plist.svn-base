<plist version="1.0">
<dict>
	<key>content</key>
	<string>display dialog "${1:text}" ¬
	${2:with icon ${3:1} ¬
	}buttons {"${5:Cancel}", "${4:OK}"} ¬
	default button "$4"
set button_pressed to button returned of result
if button_pressed is "$4" then
	${6:-- action for default button button goes here}
else
	-- action for cancel button goes here
end if</string>
	<key>name</key>
	<string>OK/Cancel</string>
	<key>scope</key>
	<string>source.applescript</string>
	<key>tabTrigger</key>
	<string>dialog</string>
